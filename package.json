{
	"name": "spotify-lite",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "vite build && vite build --ssr",
		"dev": "vite dev",
		"start": "remix-serve ./build/index.js",
		"typecheck": "tsc",
		"check": "biome check ./app",
		"check:apply": "biome check ./app --apply",
		"lint": "biome lint ./app",
		"lint:apply": "biome lint ./app --apply",
		"format": "biome format --write ./app"
	},
	"dependencies": {
		"@remix-run/node": "^2.3.1",
		"@remix-run/react": "^2.3.1",
		"@remix-run/serve": "^2.3.1",
		"@unocss/reset": "^0.58.0",
		"isbot": "^3.6.8",
		"pathe": "^1.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remix-auth": "^3.6.0",
		"remix-auth-spotify": "^1.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.4.1",
		"@remix-run/dev": "^2.3.1",
		"@remix-run/eslint-config": "^2.3.1",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"eslint": "^8.38.0",
		"remix-flat-routes": "^0.6.4",
		"typescript": "^5.1.6",
		"unocss": "^0.58.0",
		"vite": "^5.0.0",
		"vite-tsconfig-paths": "^4.2.1"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
